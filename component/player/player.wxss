/*播放器*/
.player{
  width:100%;
  height:100rpx;
  position:fixed;
  bottom:0;
  display:flex;
  background:rgba(131,180,171,.7);
}
.player .songPic{
  width:80rpx;
  height:80rpx;
  border-radius:6rpx;
  margin:10rpx;
}
.player>view{
  display:flex;
  flex-direction:column;
  height:80rpx;
  width:60%;
  margin:10rpx;
}
.player .songSinger{
  font-size:12px;
  color:#eee;
  margin-top:10rpx;
}
.player .songState,.songList{
  width:60rpx;
  height:60rpx;
  margin:20rpx;
}