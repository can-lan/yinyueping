/*播放器*/
.player{
  width:100%;
  height:100rpx;
  position:fixed;
  bottom:0;
  display:flex;
  background:rgba(131,180,171,.7);
}
.player .coverImgUrl{
  width:80rpx;
  height:80rpx;
  border-radius:6rpx;
  margin:10rpx;
}
.player .info{
  display:flex;
  flex-direction:column;
  width:50%;
  height:80rpx;
  margin:10rpx;
}
.singer{
  font-size:12px;
  color:#eee;
  margin-top:10rpx;
}
.player .btn{
  display: flex;
  justify-content:space-around;
  width:50%;
}
.prev,.play,.next,.list{
  width:60rpx;
  height:60rpx;
  margin-top:20rpx;
}