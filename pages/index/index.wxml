<!--轮播图-->
<swiper indicator-dots="true" autoplay="true" interval="3000" duration="1000">
  <block wx:for="{{banner}}" wx:key="index">
    <swiper-item>
      <image src="{{item}}" mode='aspectFit' lazy-load='true'/>
    </swiper-item>
  </block>
</swiper>
