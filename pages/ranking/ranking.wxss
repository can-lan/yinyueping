.rank{
  width:100%;
  height:145rpx;
  padding:10rpx;
  display:flex;
  font-size:12px;
  color:#eee;
  border-bottom:1px solid rgba(204,204,204,.2);
}
.rank>image{
  width:145rpx;
  height:145rpx;
  border-radius:6rpx;
}
.rank>view{
  display:flex;
  flex-direction:column;
  margin:10rpx 0 0 20rpx;
}
.rank>view>text{
  margin-bottom:15rpx;
}
